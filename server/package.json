    "redis:reset": "node tests/reset-rate-limit.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "mysql2": "^3.7.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "perf_hooks": "^0.0.1",
    "rate-limit-redis": "^4.2.2",
    "redis": "^5.8.3",
    "sanitize-html": "^2.14.0",
    "sequelize": "^6.35.2",
    "sitemap": "^8.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
