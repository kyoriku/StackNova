{
  "name": "stacknova-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "watch": "NODE_ENV=development nodemon server.js",
    "seed": "node seeds/seed.js",
    "test:rate-limit": "node tests/rate-limit.js",
    "test:benchmark": "node tests/benchmark.js",
    "test:load": "k6 run tests/k6-load-test.js",
